{
    "name": "Dropbox Upload",
    "version": "1",
    "slug": "dropbox_upload",
    "description": "Upload snapshots to Dropbox!",
    "startup": "application",
    "url": "https://github.com/d0ugal/hassio-dropbox-upload",
    "boot": "auto",
    "hassio_api": null,
    "homeassistant_api": null,
    "map": [
        "backup"
    ],
    "options": {
        "access_token": "<YOUR_ACCESS_TOKEN>",
        "dropbox_dir": "<DROPBOX_UPLOAD_LOCATION>"
    },
    "schema": {
        "access_token": "str",
        "dropbox_dir": "str",
        "debug": "bool?"
    }
}

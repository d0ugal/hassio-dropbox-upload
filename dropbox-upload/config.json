{
    "name": "Dropbox Upload",
    "version": "1.1.4",
    "slug": "dropbox_upload",
    "description": "Upload snapshots to Dropbox!",
    "startup": "application",
    "url": "https://github.com/d0ugal/hassio-dropbox-upload",
    "boot": "auto",
    "hassio_api": true,
    "hassio_role": "manager",
    "homeassistant_api": true,
    "image": "d0ugal/hassio-dropbox-upload-{arch}",
    "map": [
        "backup"
    ],
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "i386"
    ],
    "options": {
        "access_token": "<YOUR_ACCESS_TOKEN>",
        "dropbox_dir": "/snapshots",
        "keep": 10,
        "mins_between_backups": 60,
        "filename": "snapshot_slug",
        "debug": false
    },
    "schema": {
        "access_token": "match(^!secret [a-zA-Z0-9_\-]+$|^[a-zA-Z0-9\\d](?:[a-zA-Z0-9\\d]|-(?=[a-zA-Z0-9\\d])){0,32}$)",
        "dropbox_dir": "str",
        "keep": "int?",
        "mins_between_backups": "int?",
        "filename": "str",
        "debug": "bool?"
    }
}
